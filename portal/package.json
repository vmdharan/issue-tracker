{
    "name": "portal",
    "version": "1.0.0",
    "description": "User portal for the issue tracker.",
    "source": "src/index.html",
    "scripts": {
        "prebuild": "run-s clean",
        "format": "prettier --write \"**/*.{ts,tsx,css,scss}\"",
        "lint": "eslint src/",
        "lintfix": "eslint src/ --fix",
        "tidyup": "npm run format && npm run lint",
        "clean": "rimraf ./dist",
        "watch": "parcel watch src/index.html",
        "start": "parcel src/index.html --open",
        "build": "parcel build src/index.html --no-source-maps",
        "analyse": "parcel build src/index.html --reporter @parcel/reporter-bundle-analyzer",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [],
    "author": "vmdharan",
    "license": "ISC",
    "@parcel/transformer-sass": {
        "cssModules": true
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.19.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@parcel/reporter-bundle-analyzer": "^2.10.3",
        "@parcel/transformer-sass": "^2.10.3",
        "@types/react": "^18.2.38",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.33.2",
        "npm-run-all": "^4.1.5",
        "parcel": "^2.10.3",
        "prettier": "^3.1.0",
        "process": "^0.11.10",
        "rimraf": "^5.0.5",
        "typescript": "^5.3.2"
    }
}
